<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Verify Rfp by Changing account role">
 <listeners>
		<listener class-name="com.green.rfp.qa.utility.CustomReporterListener"/>
		<listener class-name="com.green.rfp.qa.utility.ExtentTestNGIReporterListener"/>
 </listeners> 
  <test  name="Verify RFP by Changing Vendor account role permission to SME">
  <parameter name="browser" value="chrome"/>
  <parameter name="selenium.url" value="https://uat.thegreenrfp.com/myaccount/rfps"/>
  <classes>
      <class name="com.green.rfp.qa.rfp.testcases.VendorRFPOwnerCanGoIntoHisAccount"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
